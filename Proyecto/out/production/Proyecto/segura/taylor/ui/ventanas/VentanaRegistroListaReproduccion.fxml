<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="546.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="segura.taylor.controlador.interfaz.listaReproduccion.ControladorRegistroListaReproduccion">
    <children>
        <VBox alignment="CENTER" prefHeight="73.0" prefWidth="663.0">
            <children>
                <Label fx:id="lblTitulo" prefHeight="73.0" prefWidth="272.0" text="Registro de Lista de Reproducción" textAlignment="CENTER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <VBox prefHeight="288.0" prefWidth="540.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox prefHeight="408.0" prefWidth="670.0">
                    <children>
                        <VBox alignment="TOP_CENTER" prefHeight="162.0" prefWidth="670.0">
                            <children>
                                <Label text="Imagen" />
                                <ImageView fx:id="imagenFondo" fitHeight="106.0" fitWidth="195.0" pickOnBounds="true" preserveRatio="true" />
                                <Button fx:id="btnSeleccionarImagen" mnemonicParsing="false" onAction="#seleccionarImagen" prefHeight="52.0" prefWidth="76.0" text="Seleccionar" />
                            </children>
                        </VBox>
                        <Label text="Nombre" />
                        <TextField fx:id="txtNombre" promptText="Nombre" />
                        <Label text="Descripción" />
                  <TextArea fx:id="txtDescripcion" prefHeight="139.0" prefWidth="670.0" promptText="Descripción" />
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="195.0" prefWidth="670.0" spacing="10.0">
                    <children>
                        <Button fx:id="btnRegistrarModificar" mnemonicParsing="false" prefHeight="32.0" prefWidth="144.0" text="Registrar" />
                        <Button fx:id="btnCerrar" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cerrar" prefHeight="32.0" prefWidth="144.0" text="Volver" />
                    </children>
                </VBox>
            </children></VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
    </padding>
</VBox>
