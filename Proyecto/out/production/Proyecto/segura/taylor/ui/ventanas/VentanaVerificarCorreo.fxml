<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="291.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="67.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="604.0" text="Verificacion de correo" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="99.0" prefWidth="440.0" spacing="5.0">
         <children>
            <Label text="Hemos enviado un código a su correo, ingréselo para continuar" />
            <TextField fx:id="txtCodigo" prefHeight="41.0" prefWidth="400.0" promptText="Código" />
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </VBox>
      <VBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button fx:id="btnVerificar" mnemonicParsing="false" prefHeight="47.0" prefWidth="131.0" text="Verificar" />
            <Button fx:id="btnVolver" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="131.0" text="Volver" />
         </children>
      </VBox>
   </children>
</VBox>
